{
  "Serilog": {
    "Using": [ "Serilog.Exceptions", "Serilog", "Serilog.Sinks.Async", "Serilog.Sinks.Trace", "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Information",
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "./logs/log_.json",
                "rollingInterval": "Day",
                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
                "restrictedToMinimumLevel": "Verbose"
              }
            }
          ]
        }
      },
      {
        "Name": "Trace",
        "Args": {
          "restrictedToMinimumLevel": "Verbose",
          "outputTemplate": "[{Timestamp:HH:mm:ss fffff} {MachineName} {ThreadId} {Level:u3}] {Message:lj}{NewLine}{Exception} {TestArg1}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Verbose",
          "outputTemplate": "[{Timestamp:HH:mm:ss fffff} {MachineName} {ThreadId} {Level:u3}] {Message:lj}{NewLine}{Exception} {TestArg2}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Environment": "Local Test",
      "TestArg1": "Test1",
      "TestArg2": "Test2"
    }
  },
  "StartupSetting": {
    "ScreenWidth": 1024,
    "ScreenHeight": 768,
    "AppName": "K.PEF.Core Example"
  },
  "TestValue": "Hello World."
}
